#include<stdio.h>
#include<math.h>
int main()
{
	int a;
	float b;
	char c;
	double d;
	printf("%d\n",sizeof(a));
	printf("%f\n",sizeof(a));
	printf("%c\n",sizeof(a));
	printf("%lf\n",sizeof(a));
	printf("%lu\n",sizeof(a));
	printf("-------------------------------------\n");
	printf("%d\n",sizeof(b));
	printf("%f\n",sizeof(b));
	printf("%c\n",sizeof(b));
	printf("%lf\n",sizeof(b));
	printf("%lu\n",sizeof(b));
	printf("-------------------------------------\n");
	printf("%d\n",sizeof(c));
	printf("%f\n",sizeof(c));
	printf("%c\n",sizeof(c));
	printf("%lf\n",sizeof(c));
	printf("%lu\n",sizeof(c));
	printf("-------------------------------------\n");
	printf("%d\n",sizeof(d));
	printf("%f\n",sizeof(d));
	printf("%c\n",sizeof(d));
	printf("%lf\n",sizeof(d));
	printf("%lu\n",sizeof(d));
	printf("-------------------------------------\n");
	
	
	

	
	
	return 0;
	
	
}
